set xrange [10:20]
set terminal pngcairo enhanced color lw 2 size 800,600 font 'Arial-Bold'
set output "CwndA-both.png"
set xlabel "Time (sec)" font "Verdana"
set ylabel "Cwnd (Packets)" font "Verdana"
set key outside;
set key right top;
plot "2_format.plotme" title "A-linux" with lines lw 1.5, "A.plotme" title "A-ns3" with lines lw 1.5

